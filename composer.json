{
  "name": "minutcz/tree",
  "type": "library",
  "description": "Library for working with jstree in Nette Framework",
  "keywords": [
    "jstree",
    "nette",
    "nette-framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Milan Nutil",
      "homepage": "https://www.minut.cz"
    }
  ],
  "autoload": {
    "psr-4": {
      "MiNutCz\\Tree\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiNutCz\\Tree\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "nette/application": "^3.1",
    "nette/utils": "^4.0"
  },
  "require-dev": {
    "nette/database": "^3.1",
    "nette/tester": "^2.5.0",
    "phpstan/phpstan-nette": "^1.2.0",
    "ninjify/coding-standard": "^0.12.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
